{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">번호</th>
        <th scope="col">제목</th>
        <th scope="col">내용</th>
        <th scope="col">작성자</th>
    </tr>
    </thead>
    <tbody>
    {{#records}}
        <tr>
            <th scope="row">{{id}}</th>
            <td><a href="/notice/{{id}}">{{title}}</a></td>
            <td>{{contents}}</td>
            <td>{{author}}</td>
        </tr>
    {{/records}}
    </tbody>
</table>

{{>layouts/footer}}